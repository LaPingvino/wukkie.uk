{
  "name": "wukkie.uk",
  "version": "0.1.0",
  "description": "A proof-of-concept bug tracker for the world, built on Bluesky/ATProto",
  "main": "dist/worker.js",
  "scripts": {
    "dev": "concurrently \"npm run build:frontend:watch\" \"wrangler dev\"",
    "build": "npm run build:frontend && npm run build:worker",
    "build:frontend": "vite build",
    "build:frontend:watch": "vite build --watch",
    "build:worker": "tsc src/worker/index.ts --outDir dist --target ES2022 --module ESNext --moduleResolution bundler",
    "deploy": "npm run build && wrangler deploy",
    "deploy:dev": "npm run build && wrangler deploy --env dev",
    "start": "wrangler dev --local",
    "preview": "wrangler dev --remote",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "atproto",
    "bluesky",
    "issue-tracking",
    "civic-engagement",
    "cloudflare-workers"
  ],
  "author": "Joop Kiefte",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241011.0",
    "@types/leaflet": "^1.9.8",
    "concurrently": "^8.2.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "wrangler": "^3.22.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LaVLaS/wukkie.uk.git"
  },
  "homepage": "https://wukkie.uk",
  "bugs": {
    "url": "https://github.com/LaVLaS/wukkie.uk/issues"
  }
}
